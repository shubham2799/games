<%- include("./partials/header") %>
<link rel="stylesheet" type="text/css" href="2048.css">

<div class="container">
	<div class="inner">
		<p class="h"><strong>2048</strong></p>
		<div class="row">
			<div class="col-lg-7">
				<h2 class="sc">Score : <span class="score">0</span></h2>
				<table>
					<tr>
						<td class="c00 f1"></td>
						<td class="c01 f1"></td>
						<td class="c02 f1"></td>
						<td class="c03 f1"></td>
					</tr>

					<tr>
						<td class="c10 f1"></td>
						<td class="c11 f1"></td>
						<td class="c12 f1"></td>
						<td class="c13 f1"></td>
